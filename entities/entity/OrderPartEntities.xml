<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="OrderPart" package="tutorial">
        <field name="ORDER_ID" type="id" is-pk="true" required="true"/>
        <field name="ORDER_PART_SEQ_ID" type="id" is-pk="true" required="true"/>
        <field name="PART_NAME" type="text-medium" default="null"/>
        <field name="STATUS_ID" type="text-short" default="null"/>
        <field name="VENDOR_PARTY_ID" type="text-short" default="null"/>
        <field name="CUSTOMER_PARTY_ID" type="text-short" default="null"/>
        <field name="PART_TOTAL" type="number-decimal" default="null"/>
        <field name="FACILITY_ID" type="text-short" default="null"/>
        <field name="SHIPMENT_METHOD_ENUM_ID" type="text-short" default="null"/>
    </entity>
</entities>