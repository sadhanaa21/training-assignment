<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="Product" package="tutorial">
        <field name="PRODUCT_ID" type="id" is-pk="true" required="true"/>
        <field name="OWNER_PARTY_ID" type="text-short" default="null"/>
        <field name="PRODUCT_NAME" type="text-medium" default="null"/>
        <field name="DESCRIPTION" type="text-long" default="null"/>
        <field name="CHARGE_SHIPPING" type="text-short" default="null"/>
        <field name="RETURNABLE" type="text-short" default="null"/>
    </entity>
</entities>