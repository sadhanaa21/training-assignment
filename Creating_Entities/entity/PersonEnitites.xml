<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="Person" package="tutorial">
        <field name="PARTY_ID" type="id" is-pk="true" required="true"/>
        <field name="SALUTATION" type="text-medium" default="null"/>
        <field name="FIRST_NAME" type="text-medium" default="null"/>
        <field name="MIDDLE_NAME" type="text-medium" default="null"/>
        <field name="LAST_NAME" type="text-medium" default="null"/>
        <field name="GENDER" type="text-short" default="null"/>
        <field name="BIRTH_DATE" type="date" default="null"/>
        <field name="MARITAL_STATUS_ENUM_ID" type="text-short" default="null"/>
        <field name="EMPLOYMENT_STATUS_ENUM_ID" type="text-short" default="null"/>
        <field name="OCCUPATION" type="text-medium" default="null"/>
    </entity>
</entities>