<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="OrderHeader" package="tutorial">
        <field name="ORDER_ID" type="id" is-pk="true" required="true"/>
        <field name="ORDER_NAME" type="text-medium" default="null"/>
        <field name="PLACED_DATE" type="date" default="null"/>
        <field name="APPROVED_DATE" type="date" default="null"/>
        <field name="STATUS_ID" type="text-short" default="null"/>
        <field name="CURRENCY_UOM_ID" type="text-short" default="null"/>
        <field name="PRODUCT_STORE_ID" type="text-short" default="null"/>
        <field name="SALES_CHANNEL_ENUM_ID" type="text-short" default="null"/>
        <field name="GRAND_TOTAL" type="number-decimal" default="null"/>
    </entity>
</entities>